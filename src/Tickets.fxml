<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.TicketsController">
   <children>
      <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="500.0" prefWidth="700.0">
         <children>
            <Button fx:id="butLogout" layoutX="569.0" layoutY="56.0" mnemonicParsing="false" onAction="#Logout" prefHeight="25.0" prefWidth="90.0" text="Log out" />
            <Button fx:id="butHome" layoutX="569.0" layoutY="98.0" mnemonicParsing="false" onAction="#Home" prefHeight="25.0" prefWidth="90.0" text="Home" />
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="486.0" prefWidth="555.0">
               <children>
                  <Label layoutX="13.0" layoutY="-3.0" text="Purchase history" />
                  <ListView fx:id="purchaseList" layoutX="13.0" layoutY="29.0" prefHeight="445.0" prefWidth="256.0" />
                  <Label fx:id="purchaseInfo" layoutX="277.0" layoutY="29.0" onMouseClicked="#selectPurchase" prefHeight="457.0" prefWidth="267.0" text="Purchase info" textAlignment="CENTER" />
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
